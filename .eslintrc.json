{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "node": true,
      "jasmine": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12
    },
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
      "no-console": "off",
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ],
      "func-names": "off",
      "radix":"off"
    }
  }
